<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.CuboCuadradoControlador">
   <children>
      <AnchorPane layoutX="24.0" layoutY="31.0" prefHeight="138.0" prefWidth="350.0">
         <children>
            <Label fx:id="labIngreseNumero" layoutX="122.0" layoutY="23.0" text="Ingrese un número: " />
            <TextField fx:id="txtNumero" layoutX="60.0" layoutY="56.0" prefHeight="26.0" prefWidth="230.0" promptText="Ingrese el número" />
            <Label fx:id="lbResultado" layoutX="107.0" layoutY="106.0" text="Resultado: " />
            <Label fx:id="labMostrarResultado" layoutX="175.0" layoutY="106.0" text="Label" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="25.0" layoutY="182.0" prefHeight="104.0" prefWidth="350.0">
         <children>
            <Button fx:id="btnCuadrado" layoutX="119.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickCuadrado" text="Calcular cuadrado" />
            <Button fx:id="btnCubo" layoutX="131.0" layoutY="63.0" mnemonicParsing="false" onAction="#clickCubo" text="Calcular cubo" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
